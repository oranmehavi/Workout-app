                android:layout_marginBottom="10dp"
                android:textColor="@color/greyish"
                android:textAllCaps="false"
                android:layout_weight="0.3"
                android:layout_marginHorizontal="5dp"
                android:text="@string/pick_image"/>
        </TableRow>

